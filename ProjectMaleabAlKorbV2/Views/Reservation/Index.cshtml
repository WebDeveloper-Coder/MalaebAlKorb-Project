
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_PublicPage.cshtml";
}

<!------------------------Panel Finder Terrain --------------------->
<div class="container">
    <div id="loginbox" class="mainbox col-md-4 col-md-offset-3 col-sm-4 col-sm-offset-2 col-xs-12">
        <div class="panel panel-info">
            <div class="panel-heading">
                <div class="panel-title">FIND YOUR PLAY</div>

            </div>

            <div style="padding-top:30px" class="panel-body">

                <div style="display:none" id="login-alert" class="alert alert-danger col-sm-12"></div>

                <form id="loginform" class="form-horizontal" role="form">

                    <div style="margin-bottom: 25px" class="input-group">
                        <span class="input-group-addon"><i class="glyphicon glyphicon-map-marker"></i></span>
                        <select class="form-control combobox_ville">
                            <option value="kech">Marrakech</option>
                            <option value="casa">Casablanca</option>
                            <option value="rabat">Rabat</option>
                            <option value="tanger">Tanger</option>

                        </select>
                    </div>

                    <div style="margin-bottom: 25px" class="input-group">
                        <span class="input-group-addon"><i class="glyphicon glyphicon-paperclip"></i></span>
                        <input id="place_playe" type="text" class="form-control" name="place_playe" placeholder="Place for playe">
                    </div>


                    <div style="margin-top:10px" class="form-group">

                        <div class="col-sm-12 controls">
                            <button id="btn-login" href="#" class="btn btn-success btn-block">Find Place  </button>


                        </div>
                    </div>



                </form>



            </div>
        </div>
    </div>
</div>
<!------------------------Panel Finder Terrain--------------------->
